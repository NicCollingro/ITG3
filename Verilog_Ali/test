#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55be7e06d5f0 .scope module, "test1" "test1" 2 16;
 .timescale 0 0;
v0x55be7e0924c0_0 .net "a", 0 0, v0x55be7e0921f0_0;  1 drivers
v0x55be7e092580_0 .net "b", 0 0, v0x55be7e0922b0_0;  1 drivers
v0x55be7e092650_0 .net "c", 0 0, v0x55be7e092350_0;  1 drivers
v0x55be7e092750_0 .var "clk", 0 0;
S_0x55be7e06d780 .scope module, "sch" "schaltung" 2 20, 2 2 0, S_0x55be7e06d5f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 1 "s";
    .port_info 2 /OUTPUT 1 "t";
    .port_info 3 /OUTPUT 1 "u";
v0x55be7e07f6d0_0 .net "clk", 0 0, v0x55be7e092750_0;  1 drivers
v0x55be7e0921f0_0 .var "s", 0 0;
v0x55be7e0922b0_0 .var "t", 0 0;
v0x55be7e092350_0 .var "u", 0 0;
E_0x55be7e07e2e0 .event posedge, v0x55be7e07f6d0_0;
    .scope S_0x55be7e06d780;
T_0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55be7e0921f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55be7e0922b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55be7e092350_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x55be7e06d780;
T_1 ;
    %wait E_0x55be7e07e2e0;
    %load/vec4 v0x55be7e0921f0_0;
    %load/vec4 v0x55be7e0922b0_0;
    %xor;
    %load/vec4 v0x55be7e092350_0;
    %xor;
    %assign/vec4 v0x55be7e0921f0_0, 0;
    %load/vec4 v0x55be7e0921f0_0;
    %load/vec4 v0x55be7e0922b0_0;
    %xor;
    %load/vec4 v0x55be7e092350_0;
    %xor;
    %load/vec4 v0x55be7e092350_0;
    %or;
    %assign/vec4 v0x55be7e0922b0_0, 0;
    %load/vec4 v0x55be7e0922b0_0;
    %assign/vec4 v0x55be7e092350_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55be7e06d5f0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55be7e092750_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x55be7e06d5f0;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x55be7e092750_0;
    %inv;
    %store/vec4 v0x55be7e092750_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55be7e06d5f0;
T_4 ;
    %delay 1, 0;
    %vpi_call 2 22 "$display", "%4t %b    | %b | %b | %b", $time, v0x55be7e092750_0, v0x55be7e0924c0_0, v0x55be7e092580_0, v0x55be7e092650_0 {0 0 0};
    %jmp T_4;
    .thread T_4;
    .scope S_0x55be7e06d5f0;
T_5 ;
    %vpi_call 2 25 "$display", "   T  clk | a | b | c |\012--------------------------------" {0 0 0};
    %delay 50, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "12-170.v";
